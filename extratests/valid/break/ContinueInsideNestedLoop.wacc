# Test that continue only exits the innermost loops

# Output:
# 1,2
# 1,3
# 1,1,5

# Program:

begin
  int i = 1 ;
  while i < 5
  do
    for (int j = 0; j < 5; skip)
    do
      j = j + 1 ;
      if j >= 2
      then
        continue
      fi ;
      print j ;
      print ","
    done ;
    i = i + 1 ;
    if i == 4
    then
      continue
    fi ;
    println i
  done
end
